<link rel="stylesheet" type="text/css" href="<%= assetPath('css/header.css')%>">
<header>
    <h1><%= title %></h1>
    <ul type="none">
        <% if(locals.user){%>
            <li><a href="/users/profile/<%= user.id %>"><%= user.name %></a></li>
            <li><a href="/users/signout">Sign Out</a></li>
        <% }else{ %>
            <li><a href="/users/login">Log In</a></li>
            <li><a href="/users/signup">Sign Up</a></li>
        <% } %>
    </ul>
</header>